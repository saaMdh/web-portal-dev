{
  "name": "portal",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "serve": "cross-env NODE_SCRIPT=serve vue-cli-service serve",
    "build": "npx update-browserslist-db@latest && npm run build:legacy && npm run build:modern",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "build:legacy": "cross-env BROWSERSLIST_ENV=legacy vue-cli-service build --name legacy-bundle --legacy && npx gulp css:legacy",
    "build:modern": "cross-env BROWSERSLIST_ENV=modern vue-cli-service build --name modern-bundle --modern && npx gulp css:modern",
    "lint:js": "eslint src/*.js",
    "postinstall": "patch-package",
    "test:lint": "npm run lint:js",
    "update-version:major": "gulp bump-major",
    "update-version:minor": "gulp bump-minor",
    "update-version:patch": "gulp bump-patch"
  },
